<h1>Dojo</h1>

<table>
	<thead>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
	</thead>
	<tbody>
		<% @dojos.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td><a href="/dojos/<%= dojo.id %>">Show</a></td>
            <td><a href="/dojos/<%= dojo.id %>/edit">Edit</a></td>
            <td><a href="/dojos/<%= dojo.id %>" data-method = "delete">Destroy</a></td>
		</tr>
		<% end %>
	</tbody>
</table>
<a href ="/dojos/new">New Dojo</a>