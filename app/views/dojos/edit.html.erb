<h2>Editing <%= @dojos.branch %></h2>
<form action="/dojos/<%= @dojos.id %>" method="patch">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>"/>
  <label>Branch:</label>
  <input type="text" value="<%= @dojos.branch %>"/>
  <label>Street:</label>
  <input type="text" value="<%= @dojos.street %>"/>
  <label>City:</label>
  <input type="text" value="<%= @dojos.city %>"/>
  <label>State:</label>
  <input type="text" value="<%= @dojos.state %>"/>
  <button type="submit">Update</button>

</form>
<a href="/dojos">Home</a>
<a href ="/dojos/show">Show</a>